<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/launcher_background"
    tools:context="org.activity.LauncherActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@drawable/rectangle">

        <ImageButton
            android:id="@+id/settings"
            android:layout_width="38dp"
            android:layout_height="38dp"
            android:layout_marginTop="3dp"
            android:layout_marginStart="375dp"
            android:background="@drawable/ic_settings"
            tools:ignore="ContentDescription" />
    </LinearLayout>

    <TextView
        android:id="@+id/ch34_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text=""
        android:layout_marginTop="60dp"
        android:textSize="18sp"/>

    <TextView
        android:id="@+id/Imei"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ch34_status"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:textSize="16sp"
        android:textColor="#FFFFFF"
        android:text=""/>

    <TextView
        android:id="@+id/text1"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginTop="151dp"
        android:layout_marginStart="92dp"
        android:text="低流量模式"
        android:textSize="16sp"
        android:textColor="#FFFFFF"
        tools:ignore="HardcodedText" />
    <Switch
        android:id="@+id/button1"
        android:layout_width="67dp"
        android:layout_height="25dp"
        android:layout_marginTop="150dp"
        android:layout_marginStart="245dp"
        android:checked="false"
        android:thumb="@drawable/ic_switch"
        android:track="@drawable/switch_type"
        android:switchTextAppearance="@style/SwitchTextColorTheme"/>
    <TextView
        android:id="@+id/switch1"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginTop="150dp"
        android:layout_marginStart="4dp"
        android:layout_toEndOf="@id/button1"
        android:text="关"
        android:textSize="16sp"
        android:textColor="#FFFFFF"
        tools:ignore="HardcodedText" />


    <TextView
        android:id="@+id/text2"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_below="@+id/text1"
        android:layout_marginTop="27dp"
        android:layout_marginStart="92dp"
        android:text="键盘"
        android:textSize="16sp"
        android:textColor="#FFFFFF"
        tools:ignore="HardcodedText" />
    <Switch
        android:id="@+id/button2"
        android:layout_width="67dp"
        android:layout_height="25dp"
        android:layout_marginTop="197dp"
        android:layout_marginStart="245dp"
        android:checked="false"
        android:thumb="@drawable/ic_switch"
        android:track="@drawable/switch_type"
        android:switchTextAppearance="@style/SwitchTextColorTheme"/>
