        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginTop="197dp"
        android:layout_marginStart="4dp"
        android:layout_toEndOf="@id/button2"
        android:text="关"
        android:textSize="16sp"
        android:textColor="#FFFFFF"
